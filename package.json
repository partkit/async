{
  "name": "@partkit/async",
  "version": "0.0.1",
  "description": "A library for async helpers.",
  "type": "module",
  "exports": {
    ".": "./dist/src/index.js",
    "./cancellation": "./dist/src/cancellation/index.js",
    "./cancellation/*": "./dist/src/cancellation/*.js",
    "./streams": "./dist/src/streams/index.js",
    "./streams/*": "./dist/src/streams/*.js",
    "./tasks": "./dist/src/tasks/index.js",
    "./tasks/*": "./dist/src/tasks/*.js"
  },
  "main": "./dist/src/index.js",
  "module": "dist/src/index.js",
  "files": [
    "./dist/src",
    "./src/"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' nyc mocha",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/partkit/async.git"
  },
  "keywords": [
    "async",
    "task",
    "microtask",
    "stream",
    "generator"
  ],
  "author": "Alexander Wende",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/partkit/async/issues"
  },
  "homepage": "https://github.com/partkit/async#readme",
  "devDependencies": {
    "@partkit/eslint-config": "^0.1.3",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "chai": "^4.3.0",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  }
}
